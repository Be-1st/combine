<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.personal_color.demo.dao.FileInfoDao">

    <insert id="insertFile" parameterType="com.personal_color.demo.dto.FileInfoDto" useGeneratedKeys="true" keyProperty="fileid">
        insert into fileInfo (fileid, name, path, filesize, description) values (#{name}, #{path}, #{filesize}, #{description})
    </insert>

</mapper>